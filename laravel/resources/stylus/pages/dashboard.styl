.dashboard_external
    background-color: #f3f3f3
    min-height: 900px

    .centre
        display block
        max-width: 1200px
        margin-left: auto
        margin-right: auto
        padding-top: 50px
        padding-bottom: 70px
        @media (max-width: 1230px)
            max-width: 90%

        .implements
            display: flex
            column-gap: 35px
            justify-content: space-between
            align-items: center

            .filters
                display: flex
                column-gap: 10px

            .buttons
                display: flex
                column-gap: 15px


        .super_title
            jakarta 28
            font-weight: 600
            text-align: center

        .father
            display: flex
            padding-top: 60px
            justify-content: space-between
            column-gap: 25px
            row-gap: 60px
            flex-wrap: wrap

            .card
                background-color: white
                box-shadow: 0px 1px 3px 1px rgba(0,0,0,0.2)
                width: 435px
                border-radius: 6px

                .graph_free
                    width: 370px
                    height: 370px
                    margin: 0 auto

                    .no_data
                        jakarta 18
                        text-align: center

                .infos
                    margin-top: 30px
                    margin-bottom: 20px

                    .title
                        jakarta 18
                        font-weight: 600
                        text-align: center
                        padding: 0 20px
                        margin: 0

            .card_liner
                background-color: white
                box-shadow: 0px 1px 3px 1px rgba(0,0,0,0.2)
                width: 100%
                border-radius: 6px
                background-color: rgb(255, 233, 233)
                
                .infos
                    margin-top: 30px
                    margin-bottom: 0px

                    .title
                        jakarta 18
                        font-weight: 600
                        text-align: center
                        padding: 0 20px
                        margin: 0

                .graph_liner
                    width: 100%
                    height: 300px
                    margin: 0 auto
                    background-color: rgb(255, 233, 233)

